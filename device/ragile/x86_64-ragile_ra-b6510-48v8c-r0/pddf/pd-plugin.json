{
  "XCVR": {
    "xcvr_present": {
      "i2c": {
        "valmap-SFP28": {
          "1": true,
          "0": false
        },
        "valmap-QSFP28": {
          "1": true,
          "0": false
        }
      }
    }
  },

  "PSU": {
    "psu_present": {
      "i2c": {
        "valmap": {
          "1": true,
          "0": false
        }
      }
    },

    "psu_power_good": {
      "i2c": {
        "valmap": {
          "1": true,
          "0": false
        }
      }
    },

    "psu_fan_dir": {
      "i2c": {
        "valmap": {
          "F2B": "EXHAUST",
          "B2F": "INTAKE"
        }
      }
    },
    "PSU_FAN_MAX_SPEED": "18000"
  },

  "FAN": {
    "direction": {
      "i2c": {
        "valmap": {
          "1": "INTAKE",
          "0": "EXHAUST"
        }
      }
    },
    "present": {
      "i2c": {
        "valmap": {
          "1": true,
          "0": false
        }
      }
    },
    "duty_cycle_to_pwm": "lambda dc: dc*255/100",
    "pwm_to_duty_cycle": "lambda pwm: pwm*100/255"
  }
}
